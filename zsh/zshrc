# -- GENERAL CONFIGURATION -----------------------------------------------------

export ZSH=$HOME/.artisan/zsh
export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8
 
# -- EDITOR CONFIGURATION ------------------------------------------------------
export EDITOR="vim"
export VISUAL="vim"

# -- ARTISAN ALIASES -----------------------------------------------------------
alias zshrc="$EDITOR ~/.zshrc_personal"
alias bodies="$EDITOR $ZSH/bodies"
alias bodies-bundle="$ZSH/bundle-bodies"

# -- ZSH OPTIONS ---------------------------------------------------------------
setopt CHASE_LINKS;                           # Chase links
setopt NO_BG_NICE;                            # Don't lower priority of BG tasks.

# -- PERSONAL OVERRIDES --------------------------------------------------------
if [[ -a ~/.zshrc_personal ]]; then
  . ~/.zshrc_personal
fi

# -- COMPLETION ----------------------------------------------------------------
autoload -Uz compinit 
compinit -C

# -- ANTIBODY STATIC LOADING ---------------------------------------------------
[ ! -f $ZSH/bodies.lock ] && bodies-bundle    # Generate if it doesn't exist
. $ZSH/bodies.lock

# -- ARTISAN SPECIFICS ---------------------------------------------------------
for file in $ZSH/includes/*; do
  . "$file"
done

